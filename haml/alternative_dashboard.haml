  ---
%div.row
  %div.span12.quick-links
    %ul.thumbnails(style="list-style: none;")
      %li.span2
        %a.thumbnail(href="spawn_vm.html")
          %img(alt="Spawn VM" src="static/focus/img/thumbs/spawn_vm.png")
          %div.caption
            %h5.nobr Spawn VM
      %li.span2
        %a.thumbnail(href="manage_vms.html")
          %img(alt="Manage VMs" src="static/focus/img/thumbs/manage.png")
          %div.caption
            %h5.nobr Manage VMs
      %li.span2
        %a.thumbnail(href="manage_images.html")
          %img(alt="Manage Images" src="static/focus/img/thumbs/manage.png")
          %div.caption
            %h5.nobr Manage Images
      %li.span2
        %a.thumbnail(href="upload_image.html")
          %img(alt="Register Image" src="static/focus/img/thumbs/upload.png")
          %div.caption
            %h5.nobr Register Image
      %li.span2
        %a.thumbnail(href="projects.html")
          %img(alt="Projects Summary" src="static/focus/img/thumbs/summary.png")
          %div.caption
            %h5.nobr Projects Summary
      %li.span2
        %a.thumbnail(href="members.html")
          %img(alt="Members" src="static/focus/img/thumbs/members.png")
          %div.caption
            %h5.nobr Members
%hr
%div.row(style="vertical-align: top")
  {% for i in page.link_groups %}
  %div.span3.nobr
    %div.hide-under-980px
      %table
        %tr
          %td
            {{ i.key }}
          %td
            %table.curly_brace
              %tr
                %td.a &nbsp;
              %tr
                %td &nbsp;
              %tr
                %td.c &nbsp;
              %tr
                %td &nbsp;
              %tr
                %td.e &nbsp;
          %td.links_group
            %ul
              {% for a in i.links %}
              %li 
                %a(href="{{ a.href }}") {{ a.title }}
              {% endfor %}
    %div.show-under-980px.row
      %h5.span1
        {{ i.key }}
    %div.show-under-980px.row
      {% for a in i.links %}
      %a.thumbnail.span1(href="{{ a.href }}" style="text-align: center; width: 140px;height: 80px; line-height: 80px; float: left")
        {{ a.title }}
      {% endfor %}
  {% endfor %}
